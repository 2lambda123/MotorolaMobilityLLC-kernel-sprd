/{
	reserved-memory {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		/* must be 64M aligned */
		sml_reserved: sml-mem@b0000000 {
			reg = <0x0 0xb0000000 0x0 0x00020000>;
		};

		tos_reserved: tos-mem@b0020000 {
			reg = <0x0 0xb0020000 0x0 0x03fe0000>;
		};

		super_memdisk_reserved: super_memdisk_reserved@100001000 {
			reg = <0x1 0x00001000 0x0 0x7fff0000>;
		};
	};

	sprd-memdisk {
		compatible = "sprd,memdisk";

		super {
			memory-region = <&super_memdisk_reserved>;
			label = "super";
		};
	};
};
