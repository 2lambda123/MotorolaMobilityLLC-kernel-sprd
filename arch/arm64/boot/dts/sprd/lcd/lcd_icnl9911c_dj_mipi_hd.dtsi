/ {
	fragment {
		target-path = "/";
		__overlay__ {
			lcds {
				lcd_icnl9911c_dj_mipi_hd: lcd_icnl9911c_dj_mipi_hd {

					sprd,dsi-work-mode = <1>; /* video burst mode*/
					sprd,dsi-lane-number = <4>;
					sprd,dsi-color-format = "rgb888";

					sprd,phy-bit-clock = <640000>;	/* kbps */
					sprd,phy-escape-clock = <20000>;/* kHz */
					sprd,width-mm = <68>;
					sprd,height-mm = <121>;


					sprd,esd-check-enable = <0>;
					sprd,esd-check-mode = <1>;
					sprd,esd-check-period = <2000>;
					sprd,esd-check-regs = <0xA>;
					sprd,esd-return-code = <0x9c>;

					sprd,reset-on-sequence = <1 10>, <0 10>, <1 10>;
					sprd,reset-off-sequence = <1 10>;

					sprd,initial-command = [

						39 00 00 03 F1 5A 59
						39 00 00 03 F0 A5 A6

						39 00 00 03 51 00 00
						23 00 00 02 53 2C
						23 00 00 02 55 01
						23 00 00 02 35 00
						13 78 00 01 11
						13 14 00 01 29
						23 00 00 02 26 01
					];
					sprd,sleep-in-command = [
						23 00 00 02 26 08
						13 14 00 01 28
						13 78 00 01 10
					];

					sprd,sleep-out-command = [
						13 78 00 01 11
						13 14 00 01 29
						23 00 00 02 26 01
					];
					oled-backlight {
						default-brightness-level = <80>;
						sprd,max-level = <255>;
						brightness-levels = [
							39 00 00 03 51 FF 0E
						];
					};

					display-timings {
						native-mode = <&icnl9911c_timing0>;
						icnl9911c_timing0: timing0 {
							clock-frequency = <96000000>;
							hactive = <720>;
							vactive = <1600>;
							hback-porch = <90>;
							hfront-porch = <90>;
							vback-porch = <32>;
							vfront-porch = <124>;
							hsync-len = <4>;
							vsync-len = <4>;
						};
					};
				};
			};
		};
	};
};
