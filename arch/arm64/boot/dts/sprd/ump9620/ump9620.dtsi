&adi_bus {
	#address-cells = <1>;
	#size-cells = <0>;

	ump9620_pmic: pmic@0 {
		compatible = "sprd,ump9620";
		reg = <0x0>;
		spi-max-frequency = <26000000>;
		interrupt-controller;
		#interrupt-cells = <2>;
		interrupts = <GIC_SPI 230 IRQ_TYPE_LEVEL_HIGH>;
		#address-cells = <1>;
		#size-cells = <0>;

	};
};

#include "regulator.dtsi"

