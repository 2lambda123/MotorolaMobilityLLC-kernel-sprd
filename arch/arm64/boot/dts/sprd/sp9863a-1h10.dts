/dts-v1/;

#include "sc9863a/sc9863a.dtsi"
#include "sc2721/sc2721.dtsi"

/ {
	model = "Spreadtrum SP9863A-1H10 Board";

	compatible = "sprd,sp9863a-1h10", "sprd,sc9863a";

	aliases {
		serial0 = &uart0;
		serial1 = &uart1;
	};

	memory@80000000 {
		device_type = "memory";
		reg = <0x0 0x80000000 0x0 0x80000000>;
	};

	chosen {
		stdout-path = "serial1:115200n8";
		bootargs = "earlycon console=ttyS1,115200n8 loop.max_part=7 loglevel=1
		init=/init root=/dev/ram0 rw printk.devkmsg=on dummy_hcd.num=0 initcall_debug=1
		vmalloc=300M";
	};
};

&uart0 {
	status = "okay";
};

&uart1 {
	status = "okay";
};
