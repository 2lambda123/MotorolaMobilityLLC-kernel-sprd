/dts-v1/;
/plugin/;

#include <dt-bindings/input/input.h>
#include <dt-bindings/gpio/gpio.h>

/ {
	fragment {
		target-path = "/";
		__overlay__ {
			sprd-wlan {
				compatible = "sprd,sc2355-wifi";
			};

			gpio-keys {
				compatible = "gpio-keys";

				key-volumedown {
					label = "Volume Down Key";
					linux,code = <KEY_VOLUMEDOWN>;
					gpios = <&ap_gpio 124 GPIO_ACTIVE_LOW>;
					debounce-interval = <2>;
					wakeup-source;
				};

				key-volumeup {
					label = "Volume Up Key";
					linux,code = <KEY_VOLUMEUP>;
					gpios = <&pmic_eic 4 GPIO_ACTIVE_HIGH>;
					debounce-interval = <2>;
					wakeup-source;
				};

				key-power {
					label = "Power Key";
					linux,code = <KEY_POWER>;
					gpios = <&pmic_eic 1 GPIO_ACTIVE_LOW>;
					debounce-interval = <2>;
					wakeup-source;
				};
			};
		};
	};

	dtbo: dtbo {
		compatible = "sprd,dtbo-test";
		reg = <0x11>;
	};
};

&reserved_memory {

	fd_reserved: faceid-mem {
		reg = <0x0 0xf13ae000 0x0 0x00f00000>;
	};

	vdsp_reserved: vdsp-mem {
		reg = <0x0 0xf22ae000 0x0 0x01e00000>;
	};

	vsp_reserved: vsp-mem {
		reg = <0x0 0xf40ae000 0x0 0x07800000>;
	};

	gpu_reserved: gpu-mem {
		reg = <0x0 0xfb8ae000 0x0 0x01e5a000>;
	};
};

&i2c3 {
	status = "okay";

	touchscreen@20 {
		compatible = "synaptics,dsx";
		reg = <0x20>;
		gpios = <&ap_gpio 145 GPIO_ACTIVE_HIGH
			&ap_gpio 144 GPIO_ACTIVE_HIGH>;
	};
};
