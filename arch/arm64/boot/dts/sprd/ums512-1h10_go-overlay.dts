				charge-full-design-microamp-hours-3 = <5000000>;
				factory-internal-resistance-micro-ohms-3 = <122000>;
				ocv-capacity-table-3 = <4391000 100>, <4327000 95>, <4267000 90>,
							<4207000 85>, <4150000 80>, <4095000 75>,
							<4048000 70>, <3991000 65>, <3954000 60>,
							<3898000 55>, <3858000 50>, <3832000 45>,
							<3812000 40>, <3795000 35>, <3782000 30>,
							<3765000 25>, <3746000 20>, <3721000 15>,
							<3692000 10>, <3680000 5>, <3400000 0>;
