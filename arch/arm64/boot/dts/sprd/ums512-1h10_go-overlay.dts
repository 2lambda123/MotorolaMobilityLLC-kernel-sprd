/ {

	model = "Unisoc UMS512 1H10 GO board";
	compatible = "sprd,ums512-1h10-go";
	sprd,sc-id = "ums512 1h10 go";

	fragment {
		target-path = "/";
		__overlay__ {
			sprd-wlan {
				compatible = "sprd,sc2355-wifi";
			};

		/*	fingerprint@0 {
				compatible = "microarray,afs121";
				fpint-gpios = <&eic_async 13 0>;
			};
        */
            fingerprint_fpc {
                compatible = "sprd,fingerprint-fpc1520";
                fpsensor,eint-gpio   = <&eic_async 13 0 >;
                fpsensor,reset-gpio  = <&ap_gpio 131 0 >;
            };

            fingerprint_goodix {
                compatible = "goodix,fingerprint";
            };

			gpio-keys {
				compatible = "gpio-keys";

				key-volumedown {
					label = "Volume Down Key";
					linux,code = <KEY_VOLUMEDOWN>;
					gpios = <&ap_gpio 124 GPIO_ACTIVE_LOW>;
					debounce-interval = <2>;
					wakeup-source;
				};

				key-google {
					label = "Google Key";
					linux,code = <KEY_GOOGLE>;
					gpios = <&ap_gpio 125 GPIO_ACTIVE_LOW>;
					debounce-interval = <2>;
					wakeup-source;
				};

				key-volumeup {
					label = "Volume Up Key";
					linux,code = <KEY_VOLUMEUP>;
					gpios = <&pmic_eic 4 GPIO_ACTIVE_HIGH>;
					debounce-interval = <2>;
					wakeup-source;
				};

				key-power {
					label = "Power Key";
					linux,code = <KEY_POWER>;
					gpios = <&pmic_eic 1 GPIO_ACTIVE_LOW>;
					debounce-interval = <2>;
					wakeup-source;
				};
			};

			usb_cable: usb_cable {
				compatible = "sprd,usb_cable_eint";
				status = "okay";
				usbcable-gpio = <&ap_gpio 126 0>;
				interrupt-parent = <&ap_gpio>;
				debounce = <256000>;
			};

