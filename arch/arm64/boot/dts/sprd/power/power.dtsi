/ {
	power_debug: debuglog {
		compatible                        = "sprd,debuglog";
		sprd,pdbg-irq-domain-gic          = "GICv3";
		sprd,pdbg-irq-domain-gpio         = "sprd-gpio";
		sprd,pdbg-irq-domain-ana          = "spi4.0";
		sprd,pdbg-irq-domain-ana-eic      = "xxx";
		sprd,pdbg-irq-domain-ap-eic-dbnc  = "xx.gpio";
		sprd,pdbg-irq-domain-ap-eic-latch = "xx.gpio";
		sprd,pdbg-irq-domain-ap-eic-async = "xx.gpio";
		sprd,pdbg-irq-domain-ap-eic-sync  = "xx.gpio";
	};

	power_stat: debugstat {
		compatible = "sprd,debugstat";
	};
};
