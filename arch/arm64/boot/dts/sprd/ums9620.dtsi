		};

		aonapb_gate: aonapb-gate {
			compatible = "sprd,ums9620-aon-gate";
			sprd,syscon = <&aon_apb_regs>; /* 0x64900000 */
			clocks = <&ext_26m>;
			clock-names = "ext-26m";
