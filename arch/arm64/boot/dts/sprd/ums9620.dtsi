			clocks = <&ext_26m>;
			clock-names = "ext-26m";
			#clock-cells = <1>;
		};

		g10_pll: g10-pll {
			compatible = "sprd,ums9620-g10-pll";
			sprd,syscon = <&anlg_phy_g10_regs>; /* 0x64334000 */
