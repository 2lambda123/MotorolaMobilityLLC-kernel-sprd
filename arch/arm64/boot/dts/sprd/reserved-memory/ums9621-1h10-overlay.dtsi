&reserved_memory {
	/* Encrypt and decrypt widevine video */
	oemcrypto_reserved: oemcrypto@b4100000 {
		reg = <0x0 0xb4100000 0x0 0x01100000>;
	};

	widevine_reserved: widevine@dd400000 {
		compatible = "shared-dma-pool";
		reg = <0x0 0xdd400000 0x0 0x12c00000>;
		reusable;
	};

	fd_reserved: faceid-mem@ff9f0000 {
		reg = <0x0 0xff9f0000 0x0 0x00500000>;
	};
};

