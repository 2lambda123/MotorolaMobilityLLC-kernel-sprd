 &adi_bus {
	ump9622_pmic: pmic@2 {
		compatible = "sprd,ump9622";
		pmic-type-num = <2>;
		reg = <0x2>;
		spi-max-frequency = <26000000>;
		#address-cells = <1>;
		#size-cells = <0>;

		syscon@2000 {
			compatible = "sprd,ump9622-syscon",
				   "sprd,sc27xx-syscon";
			reg = <0x2000>;
		};
	};
};
