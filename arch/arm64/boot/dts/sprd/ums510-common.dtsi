/ {
	display-subsystem {
		compatible = "sprd,display-subsystem";
		ports = <&dpu_port>;
	};

	reserved-memory {
		#address-cells = <2>;
		#size-cells = <2>;
		ranges;

		fb_reserved: framebuffer@9e000000{
			reg = <0x0 0x9e000000 0x0 0x1000000>;
		};
	};

};
